[Unit]
Description=figtree.service
Wants=network-online.target
After=network-online.target

[Service]
User=hyperuser
Group=hyperuser
Type=simple
TimeoutStartSec=86400
TimeoutStopSec=122
RestartSec=122
Restart=on-failure
ExecStart=/var/home/hyperuser/QWOD/RESEARCH/figtreeservice.sh start
ExecStop=/var/home/hyperuser/QWOD/RESEARCH/gitupur.sh push

[Install]
WantedBy=basic.target
