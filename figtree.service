[Unit]
Description=figtree.service
Wants=network-online.target
After=network-online.target

[Service]
User=hyperuser
Group=hyperuser
Type=exec
TimeoutStartSec=86400
TimeoutStopSec=122
ExecStart=/usr/bin/bash -c "/var/home/hyperuser/QWOD/RESEARCH/figtreeservice.sh start"
ExecStop=/usr/bin/bash -c "/var/home/hyperuser/QWOD/RESEARCH/gitupur.sh push"

[Install]
WantedBy=remote-fs.target
